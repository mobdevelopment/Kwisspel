<Window x:Class="QuizGamePack.WindowManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WindowManager" Height="132" Width="300"
        DataContext="{Binding Manager, Source={StaticResource Locator}}">

    <StackPanel Margin="0,0,0,0">
        <Button Command="{Binding ShowQuizCRUDWindowCommand}" Height="50" Click="Button_Click" Content="Voeg vragen toe"/>
        <Button Command="{Binding ShowQuestionWindowCommand}" Height="50" Content="Start de quiz"/>
    </StackPanel>
</Window>
